{
  "name": "@yext/react-site-search-starter",
  "version": "1.0.1",
  "private": true,
  "dependencies": {
    "@css-modules-theme/core": "^2.3.0",
    "@restart/hooks": "^0.4.5",
    "@restart/ui": "^1.0.1",
    "@tailwindcss/forms": "^0.4.0",
    "@yext/answers-headless-react": "file:../answers-headless-react",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-collapsed": "^3.3.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.0",
    "recent-searches": "^1.0.5",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.3.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.22",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^8.3.4",
    "express": "^4.17.2",
    "generate-license-file": "^1.3.0",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.3.0",
    "sass": "^1.49.4",
    "ts-jest": "^27.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "wcag": "./tests/wcag/index.js",
    "eslint": "eslint",
    "generate-notices": "generate-license-file --input package.json --output THIRD-PARTY-NOTICES --overwrite"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1,
          "ignoreComments": true
        }
      ],
      "react/jsx-curly-brace-presence": [
        "warn",
        "never"
      ],
      "@typescript-eslint/member-delimiter-style": [
        "warn",
        {
          "multiline": {
            "delimiter": "comma",
            "requireLast": false
          },
          "singleline": {
            "delimiter": "comma",
            "requireLast": false
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "testMatch": [
      "<rootDir>/tests/*.tsx"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@yext/answers-headless-react)",
      "^.+\\.module\\.(css|sass|scss)$"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yext/react-site-search-starter.git"
  }
}
